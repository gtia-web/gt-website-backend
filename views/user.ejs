    <script src="../assets/template/chosen/docsupport/init.js" type="text/javascript" charset="utf-8"></script>

    <!-- Control Center for Black Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="../assets/template/js/black-dashboard.min.js?v=1.0.0"></script>
</body>

