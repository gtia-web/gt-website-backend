        <section id="applying_to_GTIA">
            <div class="container">
                <div class="row pt-3">
                    <div class="col-md-12">
                        <a class="collapsed" data-bs-toggle="collapse" href="#collapse-applying" role="button" aria-expanded="true"
                            aria-controls="collapse-applying">
                            <div class="d-inline-flex">
                                <h3 class="pull-left">APPLYING TO GTIA</h3>
